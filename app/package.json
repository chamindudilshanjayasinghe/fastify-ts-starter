{
  "name": "fastify-ts-starter",
  "version": "1.0.0",
  "description": "Production-ready Fastify boilerplate with TypeScript, OpenAPI, Prisma, pino, and zod.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate"
  },
  "keywords": [
    "fastify",
    "typescript",
    "openapi",
    "prisma",
    "pino",
    "zod"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/jwt": "^10.0.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^8.0.0",
    "@prisma/client": "^5.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "fastify": "^5.5.0",
    "pino": "^8.0.0",
    "zod": "^3.0.0",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
